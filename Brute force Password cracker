import random

digits=list(range(0,100000))
password=random.randint(0,100000)
eachdigit=-1
print(password)
while eachdigit!=password:
    for eachdigit in digits:
        print(eachdigit)
        if eachdigit==password:
            print("Password is found:"+str(eachdigit))
            p=str(input("Enter new password:"))
